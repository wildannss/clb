VERSION BUILD={{version}} RECORDER=FX
SET !DATASOURCE {{source}}
SET !DATASOURCE_COLUMNS {{coloumn}}
SET !DATASOURCE_LINE {{loop}}
SET !USERAGENT {{!COL5}}

TAB T=1

CLEAR

''go to datasource''
WAIT SECONDS=1
URL GOTO={{urlDownload}}/{{urlSubDownload}}/{{source}}

''download datasource''
WAIT SECONDS=1
SAVEAS TYPE=TXT FOLDER={{folderDownload}}\Datasources\ FILE={{source}}