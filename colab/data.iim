VERSION BUILD=8970419 RECORDER=FX

TAB T=1

CLEAR

''delete old datasource''
WAIT SECONDS=1
FILEDELETE NAME={{folderDownload}}\Datasources\{{source}}

''go to datasource''
WAIT SECONDS=1
URL GOTO={{urlDownload}}/{{urlDataDownload}}/{{counted}}/{{source}}

''download datasource''
WAIT SECONDS=1
SAVEAS TYPE=TXT FOLDER={{folderDownload}}\Datasources\ FILE={{source}}